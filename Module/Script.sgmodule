hostname = %APPEND% buy.itunes.apple.com, api.textnow.me, webapi.115.com, mp.weixin.qq.com, vsco.co, license.pdfexpert.com, api.picsart.com, api.meiease.cn, apic.musixmatch.com, account.wps.com, mobile-api.adguard.com
